{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.16.1",
  "engines": {
    "node": "^24.10.0"
  },
  "scripts": {
    "dev": "dotenvx run -- node --watch src/server.ts",
    "build": "vite build",
    "preview": "dotenvx run -f .env -f .env.preview --overload -- node --enable-source-maps dist/server.js",
    "test": "dotenvx run -f .env -f .env.test --overload -- pnpm run --sequential /test:/",
    "test:it": "node --run migrate -- apply --auto-approve && vitest --dir tests/graphql/Mutation/ --maxWorkers 1",
    "check": "tsgo && biome check && prettier --cache --check './**/*.{yml,yaml,md}'",
    "migrate": "dotenvx run -- atlas schema --env env",
    "generate": "dotenvx run -- kysely-codegen --config-file kysely-codegen.ts",
    "seed": "dotenvx run -- node db/seed.ts",
    "codegen": "dotenvx run -- node schemagen.ts && biome format --write schema.graphql && graphql-codegen-esm"
  },
  "dependencies": {
    "@envelop/core": "^5.0.0",
    "@escape.tech/graphql-armor": "^3.1.1",
    "@graphql-yoga/plugin-disable-introspection": "^2.7.0",
    "@whatwg-node/server-plugin-cookies": "^1.0.4",
    "bcrypt": "^6.0.0",
    "dataloader": "^2.2.2",
    "graphql": "^16.8.1",
    "graphql-fields": "^2.0.3",
    "graphql-query-complexity": "^0.12.0",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.7.0",
    "jose": "^6.0.4",
    "kysely": "^0.28.8",
    "neverthrow": "^8.2.0",
    "pg": "^8.11.3",
    "pino": "^9.6.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.56.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@dotenvx/dotenvx": "^1.14.1",
    "@faker-js/faker": "^10.0.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "5.0.0",
    "@graphql-tools/executor-http": "^3.0.1",
    "@graphql-tools/utils": "^10.8.1",
    "@types/bcrypt": "^6.0.0",
    "@types/graphql-fields": "^1.3.9",
    "@types/pg": "^8.11.2",
    "@typescript/native-preview": "7.0.0-dev.20251204.1",
    "es-toolkit": "^1.29.0",
    "kysely-codegen": "^0.19.0",
    "pino-pretty": "^13.0.0",
    "prettier": "3.6.2",
    "type-fest": "^4.27.0",
    "vite": "^7.1.5",
    "vitest": "^4.0.13"
  }
}
