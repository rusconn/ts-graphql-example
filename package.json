{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.30.1",
  "engines": {
    "node": "^24.10.0"
  },
  "scripts": {
    "dev": "dotenvx run -- node --watch src/presentation/server.ts",
    "build": "vite build",
    "preview": "dotenvx run -f .env -f .env.preview --overload -- node --enable-source-maps dist/server.js",
    "test": "dotenvx run -f .env -f .env.test --overload -- pnpm run --sequential /test:/",
    "test:ut": "vitest --config vitest.ut.config.ts",
    "test:setup": "node --run migrate -- clean --auto-approve && node --run migrate -- apply --auto-approve",
    "test:it": "vitest --config vitest.it.config.ts",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "check": "tsgo && biome check && prettier --cache --check './**/*.{yml,yaml,md}'",
    "migrate": "dotenvx run -- atlas schema --env env",
    "generate": "dotenvx run -- kysely-codegen --config-file kysely-codegen.ts",
    "seed": "dotenvx run -- node db/seed.ts",
    "codegen": "dotenvx run -- node schemagen.ts && biome format --write schema.graphql && graphql-codegen-esm"
  },
  "dependencies": {
    "@envelop/core": "^5.5.1",
    "@escape.tech/graphql-armor": "^3.2.0",
    "@graphql-yoga/plugin-disable-introspection": "^2.7.0",
    "@whatwg-node/server-plugin-cookies": "^1.0.5",
    "bcrypt": "^6.0.0",
    "dataloader": "^2.2.3",
    "graphql": "^16.12.0",
    "graphql-fields": "^2.0.3",
    "graphql-query-complexity": "^0.12.0",
    "graphql-scalars": "^1.25.0",
    "graphql-yoga": "^5.7.0",
    "jose": "^6.1.3",
    "kysely": "^0.28.11",
    "neverthrow": "^8.2.0",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.56.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@dotenvx/dotenvx": "^1.52.0",
    "@faker-js/faker": "^10.3.0",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "5.0.8",
    "@graphql-codegen/typescript-operations": "^5.0.8",
    "@graphql-codegen/typescript-resolvers": "5.1.6",
    "@graphql-tools/executor-http": "^3.1.0",
    "@graphql-tools/utils": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/graphql-fields": "^1.3.9",
    "@types/pg": "^8.16.0",
    "@typescript/native-preview": "7.0.0-dev.20260220.1",
    "@whatwg-node/cookie-store": "^0.2.3",
    "es-toolkit": "^1.44.0",
    "kysely-codegen": "^0.20.0",
    "pino-pretty": "^13.1.3",
    "prettier": "3.8.1",
    "type-fest": "^5.4.4",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
