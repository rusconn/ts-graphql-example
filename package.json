{
  "license": "MIT",
  "private": true,
  "scripts": {
    "setup": "run-s setup:*",
    "setup:migrate": "yarn migrate:reset ",
    "setup:generate": "yarn generate",
    "setup:seed": "yarn seed",
    "start": "yarn ts src/index.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "pretest": "DB_NAME=testdb yarn migrate:reset",
    "test": "NODE_ENV=test DB_NAME=testdb jest -i --silent",
    "seed": "yarn ts prisma/seed.ts",
    "generate": "prisma generate",
    "ts": "ts-node-dev -r tsconfig-paths/register --transpile-only",
    "codegen": "graphql-codegen",
    "fix": "run-s fix:*",
    "fix:generate": "prisma generate",
    "fix:typecheck": "tsc --noEmit",
    "fix:lint": "eslint . --cache --fix --ext '.js,.ts'",
    "fix:format": "prettier --write './**/*.{js,ts,json,yml,md}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.0.0",
    "@devoxa/prisma-relay-cursor-connection": "^2.1.0",
    "@graphql-tools/schema": "^8.5.0",
    "@prisma/client": "4.1.1",
    "extensible-custom-error": "^0.0.7",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.1.31",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.12.6",
    "graphql-validation-complexity": "^0.4.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pino": "^8.1.0",
    "prisma": "4.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-operations": "^2.5.1",
    "@graphql-codegen/typescript-resolvers": "^2.7.1",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": ">=8.0.1",
    "jest": "^28.1.3",
    "lint-staged": ">=13.0.3",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
