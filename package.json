{
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.5.1",
  "engines": {
    "node": "^18.16.0"
  },
  "prisma": {
    "seed": "pnpm ts prisma/seed.ts"
  },
  "scripts": {
    "dev": "dotenvx run -- pnpm ts --watch src/main.ts",
    "build": "dotenvx run -- vite build",
    "start": "dotenvx run -f .env -f .env.production --overload -- node --enable-source-maps dist/main.js",
    "test": "run-s test:ut test:it",
    "test:ut": "dotenvx run -f .env -f .env.test --overload -- vitest --dir src",
    "test:it": "dotenvx run -f .env -f .env.test --overload -- run-s test:it:*",
    "test:it:migrate": "pnpm migrate:reset --skip-seed",
    "test:it:run": "vitest --dir tests",
    "check": "run-s check:*",
    "check:typecheck": "pnpm typecheck",
    "check:lint": "pnpm lint",
    "check:format": "run-p fix:format:*",
    "check:format:biome": "pnpm format:biome",
    "check:format:prettier": "pnpm format:prettier --check",
    "fix": "run-s fix:*",
    "fix:typecheck": "pnpm typecheck",
    "fix:lint": "pnpm lint --apply",
    "fix:format": "run-p fix:format:*",
    "fix:format:biome": "pnpm format:biome --write",
    "fix:format:prettier": "pnpm format:prettier --write",
    "studio": "prisma studio",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "codegen": "graphql-codegen-esm",
    "ts": "vite-node",
    "typecheck": "tsc",
    "lint": "biome check .",
    "format:biome": "biome format .",
    "format:prettier": "prettier --cache './**/*.{yml,yaml,md}'"
  },
  "dependencies": {
    "@envelop/core": "^5.0.0",
    "@escape.tech/graphql-armor": "^2.3.2",
    "@graphql-yoga/plugin-disable-introspection": "^2.0.0",
    "bcrypt": "^5.1.1",
    "dataloader": "^2.2.2",
    "graphql": "^16.8.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.0.0",
    "kysely": "^0.27.3",
    "pg": "^8.11.3",
    "pino": "^8.21.0",
    "remeda": "^1.29.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.34.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@dotenvx/dotenvx": "^0.44.1",
    "@faker-js/faker": "^8.4.1",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "4.0.0",
    "@graphql-tools/executor-http": "^1.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-fields": "^1.3.9",
    "@types/pg": "^8.11.2",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.1.0",
    "prettier": "2.7.1",
    "prisma": "5.15.0",
    "prisma-kysely": "^1.8.0",
    "type-fest": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-node": "^1.3.1",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.6.0"
  }
}
