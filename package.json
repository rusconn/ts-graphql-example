{
  "license": "MIT",
  "private": true,
  "scripts": {
    "setup": "yarn && run-s setup:*",
    "setup:migrate": "yarn migrate:reset ",
    "setup:generate": "yarn generate",
    "setup:seed": "yarn seed",
    "dev": "nodemon --watch src --ext 'js,ts' --exec 'yarn ts src/index.ts'",
    "build": "swc src --out-dir dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist/*",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "pretest": "DB_NAME=testdb yarn migrate:reset",
    "test": "NODE_ENV=test DB_NAME=testdb jest -i --silent",
    "seed": "yarn ts prisma/seed.ts",
    "generate": "prisma generate",
    "ts": "node -r @swc-node/register",
    "codegen": "graphql-codegen",
    "fix": "run-s fix:*",
    "fix:generate": "prisma generate",
    "fix:typecheck": "tsc --noEmit",
    "fix:lint": "eslint . --cache --fix --ext '.js,.ts'",
    "fix:format": "prettier --write './**/*.{js,ts,json,yml,md}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@devoxa/prisma-relay-cursor-connection": "^2.1.0",
    "@graphql-tools/schema": "^8.5.0",
    "@prisma/client": "4.8.1",
    "bcrypt": "^5.1.0",
    "extensible-custom-error": "^0.0.7",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.1.31",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.12.6",
    "graphql-validation-complexity": "^0.4.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pino": "^8.1.0",
    "prisma": "4.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-operations": "^2.5.1",
    "@graphql-codegen/typescript-resolvers": "^2.7.1",
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.8",
    "@swc/jest": "^0.2.23",
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/graphql-validation-complexity": "^0.4.0",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": ">=8.0.1",
    "jest": "^29.2.0",
    "lint-staged": ">=13.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.1.0",
    "prettier": "2.7.1",
    "typescript": "^4.7.4"
  }
}
