{
  "license": "MIT",
  "private": true,
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.5.1"
  },
  "scriptsComments": {
    "build": "--ignore が効かない: https://github.com/swc-project/cli/issues/20"
  },
  "scripts": {
    "setup": "pnpm install && NODE_ENV=development run-s setup:*",
    "setup:migrate": "pnpm migrate:reset ",
    "setup:generate": "pnpm generate",
    "setup:seed": "pnpm seed",
    "setup:husky": "husky install",
    "dev": "NODE_ENV=development nodemon --watch src --ext 'js,ts' --exec 'pnpm ts src/main.ts'",
    "build": "swc src --ignore '**/*.test.ts' --delete-dir-on-start --copy-files --out-dir dist",
    "start": "NODE_ENV=production node --enable-source-maps dist/main.js",
    "codegen": "graphql-codegen",
    "studio": "pnpm prisma studio",
    "test": "pnpm test:ut && pnpm test:it",
    "test:ut": "NODE_ENV=test vitest --run --silent --dir src",
    "test:it": "NODE_ENV=test run-s test:it:*",
    "test:it:migrate": "DB_NAME=testdb pnpm migrate:reset",
    "test:it:run": "DB_NAME=testdb vitest --run --silent --single-thread --dir tests",
    "check": "run-s check:*",
    "check:generate": "pnpm generate",
    "check:typecheck": "pnpm typecheck",
    "check:lint": "pnpm lint",
    "check:format": "pnpm format --check",
    "fix": "run-s fix:*",
    "fix:generate": "pnpm generate",
    "fix:typecheck": "pnpm typecheck",
    "fix:lint": "pnpm lint --fix",
    "fix:format": "pnpm format --write",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "generate": "prisma generate",
    "seed": "pnpm ts prisma/seed.ts",
    "ts": "node -r @swc-node/register",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache --ext '.js,.ts'",
    "format": "prettier --cache './**/*.{js,ts,json,yml,md}'"
  },
  "dependencies": {
    "@devoxa/prisma-relay-cursor-connection": "^2.2.2",
    "@envelop/core": "^5.0.0",
    "@escape.tech/graphql-armor": "^2.3.2",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-yoga/plugin-disable-introspection": "^2.0.0",
    "@prisma/client": "5.4.2",
    "bcrypt": "^5.1.1",
    "graphql": "^16.8.1",
    "graphql-middleware": "^6.1.31",
    "graphql-scalars": "^1.17.0",
    "graphql-yoga": "^5.0.0",
    "pino": "^8.1.0",
    "prisma": "5.4.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "4.0.0",
    "@graphql-tools/executor-http": "^1.0.3",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@types/bcrypt": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": ">=8.0.1",
    "lint-staged": ">=13.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.1.0",
    "prettier": "2.7.1",
    "remeda": "^1.29.0",
    "type-fest": "^4.2.0",
    "typescript": "^4.7.4",
    "vitest": "^0.34.6"
  }
}
