{
  "license": "MIT",
  "private": true,
  "volta": {
    "node": "18.16.0",
    "pnpm": "8.5.1"
  },
  "scripts": {
    "setup": "pnpm install && NODE_ENV=development run-s setup:*",
    "setup:migrate": "pnpm migrate:reset ",
    "setup:generate": "pnpm generate",
    "setup:seed": "pnpm seed",
    "setup:husky": "husky install",
    "dev": "NODE_ENV=development nodemon --watch src --ext 'js,ts' --exec 'pnpm ts src/index.ts'",
    "build": "swc src --out-dir dist",
    "start": "NODE_ENV=production node dist/index.js",
    "clean": "rm -rf dist/*",
    "codegen": "graphql-codegen",
    "studio": "pnpm prisma studio",
    "test": "NODE_ENV=test run-s test:*",
    "test:migrate": "DB_NAME=testdb pnpm migrate:reset",
    "test:run": "DB_NAME=testdb jest -i --silent",
    "check": "run-s check:*",
    "check:generate": "pnpm generate",
    "check:typecheck": "pnpm typecheck",
    "check:lint": "pnpm lint",
    "check:format": "pnpm format --check",
    "fix": "run-s fix:*",
    "fix:generate": "pnpm generate",
    "fix:typecheck": "pnpm typecheck",
    "fix:lint": "pnpm lint --fix",
    "fix:format": "pnpm format --write",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --force",
    "generate": "prisma generate",
    "seed": "pnpm ts prisma/seed.ts",
    "ts": "node -r @swc-node/register",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache --ext '.js,.ts'",
    "format": "prettier --cache './**/*.{js,ts,json,yml,md}'"
  },
  "dependencies": {
    "@apollo/server": "^4.9.0",
    "@devoxa/prisma-relay-cursor-connection": "^2.2.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "@paljs/plugins": "^5.3.3",
    "@prisma/client": "5.1.0",
    "bcrypt": "^5.1.0",
    "extensible-custom-error": "^0.0.7",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-middleware": "^6.1.31",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "^7.5.0",
    "graphql-validation-complexity": "^0.4.2",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pino": "^8.1.0",
    "prisma": "5.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-resolvers": "4.0.0",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@swc/jest": "^0.2.26",
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/graphql-validation-complexity": "^0.4.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": ">=8.0.1",
    "jest": "^29.5.0",
    "lint-staged": ">=13.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.1.0",
    "prettier": "2.7.1",
    "typescript": "^4.7.4"
  }
}
