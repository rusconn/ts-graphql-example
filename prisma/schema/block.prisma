model Block {
  createdAt DateTime
  blockerId String   @pg.Uuid
  blockeeId String   @pg.Uuid

  blocker  User @relation("blockedBy", fields: blockerId, references: id, onDelete: Cascade)
  blocking User @relation("blocking", fields: blockeeId, references: id, onDelete: Cascade)

  @@id([blockerId, blockeeId])
}
